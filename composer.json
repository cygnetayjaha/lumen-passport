{
    "name": "cygnet/cyg-lumen-passport",
    "description": "OAuth 2.0 authorization with custom Payload with help of JWT.",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "ext-openssl": "*",
        "lcobucci/jwt": "^3.2.2",
        "psr/http-message": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3 || ^7.0",
        "zendframework/zend-diactoros": "^1.3.2",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpstan/phpstan-strict-rules": "^0.9.0",
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/cygnetayjaha/lumen-passport.git"
        }
    ],
    "authors": [
        {
            "name": "Anuj Jaha",
            "email": "er.anujjaha@gmail.com",
            "role": "Developer"
        }
    ],
    "replace": {
        "cygnet/cyg-lumen-passport": "*"
    },
    "autoload": {
        "psr-4": {
            "Cygnet\\OAuth2\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    }
}
